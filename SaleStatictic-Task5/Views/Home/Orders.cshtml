@model System.Collections.Generic.List<SaleStatictic_Task5.Models.OrderViewModel>

<div id="orders">
    <table class="table table-striped">
        <tr>
            <td>Id</td>
            <td>Date</td>
            <td>ClientName</td>
            <td>ManagerName</td>
            <td>ProductName</td>
        </tr>
        @if (!Model.Any())
        {
            <tr>
                <td colspan="3" class="text-center">There is no orders</td>
            </tr>
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Date</td>
                <td>@item.ClientName</td>
                <td>@item.ManagerName</td>
                <td>@item.ProductName</td>
            </tr>
        }
    </table>

</div>